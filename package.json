{
  "name": "n8n-nodes-resiliate-events",
  "version": "0.1.0",
  "description": "n8n trigger node for Resiliate events and data resiliency monitoring",
  "keywords": [
    "n8n-community-node-package",
    "resiliate",
    "saf.ai",
    "data-resiliency",
    "ransomware-protection",
    "filesystem-events"
  ],
  "license": "MIT",
  "homepage": "https://saf.ai/resiliate/n8n/",
  "author": {
    "name": "Ahmed Masud",
    "email": "ahmed.masud@saf.ai",
    "url": "https://saf.ai"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:safai-labs/resiliate-n8n.git",
    "homepage": "https://github.com/safai-labs/resiliate-n8n"
  },
  "bugs": {
    "url": "https://github.com/safai-labs/resiliate-n8n/issues"
  },
  "documentation": "https://docs.saf.ai/",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write nodes credentials",
    "lint": "eslint nodes credentials --ext .js,.ts",
    "lint:fix": "eslint nodes credentials --ext .js,.ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ResiliateEventsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ResiliateEvents/ResiliateEvents.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@typescript-eslint/parser": "~5.45",
    "gulp": "^4.0.2",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "express": "^4.17.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
